{
  "manifest_version": 3,
  "name": "Doc Parser",
  "version": "1.0",
  "permissions": ["activeTab", "scripting", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Run Doc Parser"
  },
  "web_accessible_resources": [
    {
      "resources": ["output.html", "output.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
